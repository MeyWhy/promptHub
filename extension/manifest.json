{
    "manifest_version": 3,
    "name": "PromptHub",
    "version": "1.0",
    "description": "Save, organize and autocomplete your favorite AI prompts",
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["storage"]
  }
  